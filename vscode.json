{
  "terminal.integrated.minimumContrastRatio": 15,
  "workbench.iconTheme": "material-icon-theme",
  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "left",
  "explorer.openEditors.visible": 0,
  "workbench.startupEditor": "newUntitledFile",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "editor.wordWrap": "on",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.enableSmartCommit": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "http.proxySupport": "off",
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "explorer.confirmDragAndDrop": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Hop Light",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontFamily": "monospace",
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "appService.showDeploySubpathWarning": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.editor.showTabs": true,
  "breadcrumbs.enabled": true,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "editor.linkedEditing": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "workbench.tree.indent": 24,
  "terminal.integrated.fontSize": 14,
  "editor.lineNumbers": "off",
  "workbench.activityBar.visible": false,
  "workbench.preferredLightColorTheme": "Hop Light",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "window.autoDetectColorScheme": true,
  "workbench.tree.renderIndentGuides": "always",
  "editor.renderIndentGuides": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "block",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.showFoldingControls": "always"
}
