{
  "terminal.integrated.minimumContrastRatio": 15,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "editor.wordWrap": "on",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.enableSmartCommit": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnType": false,
  "explorer.confirmDragAndDrop": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Hop Light",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "editor.linkedEditing": true,
  "files.autoSaveDelay": 100,
  "workbench.tree.indent": 24,
  "terminal.integrated.fontSize": 14,
  "editor.lineNumbers": "on",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "window.autoDetectColorScheme": true,
  "workbench.tree.renderIndentGuides": "always",
  "editor.cursorBlinking": "smooth",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.showFoldingControls": "always",
  "editor.overviewRulerBorder": false,
  "workbench.list.smoothScrolling": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontSize": 13,
  "git.autofetch": true,
  "workbench.colorCustomizations": {},
  "workbench.sideBar.location": "left",
  "editor.minimap.side": "left",
  "editor.minimap.enabled": false,
  "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.wrapTabs": true,
  "editor.formatOnSave": true,
  "workbench.activityBar.location": "hidden",
  "editor.scrollBeyondLastLine": false,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingComments": "never",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.codeActionWidget.includeNearbyQuickFixes": false,
  "editor.codeActionWidget.showHeaders": false,
  "editor.codeLens": false,
  "editor.colorDecorators": false,
  "editor.colorDecoratorsActivatedOn": "click",
  "editor.comments.insertSpace": false,
  "editor.comments.ignoreEmptyLines": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.detectIndentation": false,
  "editor.dropIntoEditor.enabled": false,
  "editor.experimental.asyncTokenization": false,
  "editor.experimentalWhitespaceRendering": "off",
  "editor.accessibilitySupport": "off",
  "editor.guides.bracketPairs": "active",
  "editor.guides.highlightActiveIndentation": false,
  "editor.hover.enabled": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.lightbulb.enabled": "off",
  "editor.links": false,
  "editor.parameterHints.enabled": false,
  "editor.parameterHints.cycle": false,
  "editor.pasteAs.enabled": false,
  "editor.pasteAs.showPasteSelector": "never",
  "editor.peekWidgetDefaultFocus": "editor",
  "editor.rename.enablePreview": false,
  "editor.scrollbar.scrollByPage": true,
  "editor.scrollbar.vertical": "visible",
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.stickyScroll.enabled": false,
  "editor.unicodeHighlight.includeComments": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "diffEditor.experimental.showMoves": true,
  "application.shellEnvironmentResolutionTimeout": 120
}

