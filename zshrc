# Aliases
alias cdco='cd ~/code'
alias ll='ls -lhAF'
alias v=vim
alias rf='node ~/code/pro/artelia/scripts/front-review.mjs'

# Terminal
export PATH=$PATH:~/.npm-packages/bin
export CLICLOR=1

# Node.js
export NODE_OPTIONS=--openssl-legacy-provider 
export NODE_PENDING_DEPRECATION=1
export N_PREFIX=~/.n

# zshrc
ZSH_THEME="af-magic"
source ~/.oh-my-zsh/oh-my-zsh.sh
